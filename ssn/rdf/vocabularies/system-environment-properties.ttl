sosa-env: a owl:Ontology , voaf:Vocabulary ;
  dcterms:title "Sample vocabulary of properties about the system environment"@en ;
  dcterms:description "This vocabulary defines a short list of properties about the system environment. 

  It is only for illustration purposes, as the Spatio-temporal Data on the Web Working Group does not ambition to maintain a living list of properties. Extensions of this list should be led by other organization, and publications done to relevant vocabulary portals."@en ;
  dcterms:creator [ a foaf:Agent ; foaf:name "W3C/OGC Spatio-temporal Data on the Web Working Group"@en ] ;
  dcterms:rights "Copyright 2025 W3C/OGC." ;
  dcterms:license <http://www.w3.org/Consortium/Legal/2015/copyright-software-and-document> ;
  dcterms:license <http://www.opengeospatial.org/ogc/Software> ;
  rdfs:comment "Please report any errors to the W3C Spatio-temporal Data on the Web Working Group via the SDW WG Public List public-sdw-wg@w3.org"@en ;
  dcterms:modified "2025-06-25"^^xsd:date ;
  vann:preferredNamespacePrefix "sosa-env" ;
  vann:preferredNamespaceUri "http://www.w3.org/ns/sosa/system-environment-properties#" ;
  owl:imports sosa-systems: .

sosa-env:EnvironmentTemperature a sosa:Property ;
  rdfs:label "environment temperature"@en ;
  skos:definition "The temperature of the environment of the system."@en ;
  rdfs:isDefinedBy sosa-env: .

sosa-env:EnvironmentHumidity a sosa:Property ;
  rdfs:label "environment humidity"@en ;
  skos:definition "The humidity of the environment of the system."@en ;
  rdfs:isDefinedBy sosa-env: .
  