RewriteEngine On

# 302 redirects from generic URIs to 2023 URIs for all files that existed in 2017 edition, and still exist in 2023 edition
# 302 redirects from generic URIs to 2017 URIs for all files that existed in 2017 edition, and do not exist in 2023 edition

# example:
# set up 302 redirect ssn to 2023/ssn
# ...

# 
# set up 302 redirect sampling to 2023/sampling # ./sosa/sampling existed in 2017 and exists in 2023.

301 redirect http://www.w3.org/ns/ssn/systems.* to http://www.w3.org/ns/ssn/2017/systems$1


